{
  "name": "angular-seed",
  "private": true,
  "version": "0.0.0",
  "description": "A starter project for AngularJS",
  "repository": "https://github.com/angular/angular-seed",
  "license": "MIT",
  "scripts": {
    "start": "npm run lite",
    "ut:bdd": "node node_modules/karma/bin/karma start tests/karma.bdd.conf.js # ut+cucumber",
    "cucumber:create-steps": "node ./step_factories/.bin/generate-steps.js",
    "cucumber:debug:create-steps": "node node_modules/cucumber/bin/cucumber.js --snippet-syntax step_factories/.supports/javascript_syntax.js step_factories/features/myTodo.feature",
    "lite": "lite-server -c bs-config.js"
  },
  "dependencies": {
    "shelljs": "0.7.5"
  },
  "devDependencies": {
    "angular": "1.6.1",
    "angular-cookies": "1.6.1",
    "angular-local-storage": "0.5.0",
    "angular-midway": "0.0.81",
    "angular-mocks": "1.6.1",
    "angular-resource": "1.6.1",
    "angular-route": "1.6.1",
    "angular-sanitize": "1.6.1",
    "angular-ui-sortable": "0.16.1",
    "bootstrap": "~3.3.6",
    "chai": "~3.5.0",
    "chai-as-promised": "~5.3.0",
    "colors": "~1.1.2",
    "cucumber": "1.3.1",
    "cucumber-html-reporter": "0.3.7",
    "es5-shim": "^4.5.9",
    "jquery": "3.1.1",
    "jquery-ui.1.11.1": "^1.11.1",
    "json3": "^3.3.2",
    "karma": "1.3.0",
    "karma-chai-as-promised": "~0.1.2",
    "karma-chai-sinon": "~0.1.5",
    "karma-chrome-launcher": "2.0.0",
    "karma-coverage": "^1.1.0",
    "karma-cucumber-js": "1.0.0",
    "karma-html-reporter": "~0.2.7",
    "karma-hy-html-reporter": "~0.0.14",
    "karma-mocha-reporter": "2.2.1",
    "karma-ng-html2js-preprocessor": "~1.0.0",
    "karma-read-json": "~1.1.0",
    "karma-spec-reporter": "0.0.26",
    "lite-server": "^2.2.2",
    "node-inspector": "^0.12.8",
    "sinon": "1.17.6",
    "sinon-chai": "~2.8.0"
  }
}
